<div class="page-header">
  <%= link_to rental_records_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_rental_record_path(@rental_record), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show rental_record</h1>
</div>

<dl class="dl-horizontal">
  <dt>Customer:</dt>
  <dd><%= @rental_record.customer %></dd>

  <dt>Vehicle:</dt>
  <dd><%= @rental_record.vehicle %></dd>

  <dt>Start date:</dt>
  <dd><%= @rental_record.start_date %></dd>

  <dt>End date:</dt>
  <dd><%= @rental_record.end_date %></dd>

  <dt>Lastupdated:</dt>
  <dd><%= @rental_record.lastUpdated %></dd>

  <dt>Output:</dt>
  <dd><%= @rental_record.output %></dd>

</dl>
