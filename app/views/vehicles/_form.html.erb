<%= simple_form_for(@vehicle) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :veh_reg_no %>
    <%= f.input :category %>
    <%= f.input :brand %>
    <%= f.input :name %>
    <%= f.input :daily_rate %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
